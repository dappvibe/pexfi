    "prettier": "^3.7.4",
    "prop-types": "^15.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "7.3.0",
