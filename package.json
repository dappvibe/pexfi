{
  "name": "pexfi",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run wagmi:generate && npm run build:evm && npm run build:web",
    "build:web": "vite build",
    "build:web:staging": "vite build --mode staging",
    "build:web:prod": "vite build --mode production",
    "build:evm": "hardhat compile --config hardhat.config.cjs",
    "wagmi:generate": "wagmi generate",
    "test": "vitest",
    "test:web": "vitest run",
    "test:evm": "hardhat test --config hardhat.config.cjs",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "subgraph:codegen": "graph codegen subgraph/subgraph.yaml -o subgraph/generated",
    "subgraph:build": "graph build subgraph/subgraph.yaml -o subgraph/build"
  },
