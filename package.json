{
  "name": "pexfi",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run wagmi:generate && npm run build:evm && npm run build:web",
    "build:web": "vite build",
    "build:web:staging": "vite build --mode staging",
    "build:web:prod": "vite build --mode production",
    "build:evm": "hardhat compile",
    "build:subgraph": "npx graph build subgraph/subgraph.yaml -o .cache/subgraph/build --network-file subgraph/networks.json --network",
    "docs:dev": "cd docs && npx mint dev --no-open",
    "wagmi:generate": "wagmi generate",
    "test": "vitest",
    "test:web": "vitest run",
    "test:evm": "hardhat test",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "subgraph:codegen": "npx graph codegen subgraph/subgraph.yaml -o .cache/subgraph/generated"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@dr.pogodin/react-helmet": "^3.0.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@nomicfoundation/hardhat-ignition": "^3.0.7",
    "@tanstack/query-sync-storage-persister": "^5.90.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-persist-client": "^5.90.18",
    "antd": "^6.1.4",
    "ethers": "^6.16.0",
    "graphql": "^16.12.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "viem": "^2.43.5",
    "wagmi": "^3.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@graphprotocol/graph-cli": "0.87.0",
    "@graphprotocol/graph-ts": "0.32.0",
    "@jetbrains/youtrack-scripting-api": "^2022.1.46592",
    "@nomicfoundation/hardhat-toolbox-mocha-ethers": "^3.0.2",
    "@nomicfoundation/hardhat-verify": "^3.0.9",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@uma/core": "^2.62.2",
    "@uniswap/universal-router": "^2.1.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v4-periphery": "^1.0.3",
    "@uniswap/v4-core": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@wagmi/cli": "^2.8.0",
    "@wonderland/walletless": "^1.0.0",
    "dotenv": "^17.2.3",
